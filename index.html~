<script type="text/javascript">
nowshown = "Play"

call = "n/a"

nrt1 = "http://fire.wavestreamer.com:5102/Live"
nrt1now = "NRT Radio One"
nrt1desc = "A wide range of Christian music."

fnt = "http://65.19.131.141/foxnewsradio-foxnewsaac-ibc2"
fntnow = "Fox News Talk"
fntdesc = "Talk radio from Fox News."
fntBack = "fntback.jpg"

cain = "http://oom-cmg.streamguys1.com/atl750/cain247im-sgplayer-aac"
cainnow = "Herman Cain"
caindesc = "Listen to Herman Cain 24/7."
function isBetween(S, E) {
	Start = S.split(":")
	Start[0] = parseInt(Start[0])
	StartM = Start[1].split(" ")
	Start[1] = StartM[0]
	
	End = E.split(":")
	End[0] = parseInt(End[0])
	EndM = End[1].split(" ")
	End[1] = EndM[0]
	
	if(Start[0] == 12 && StartM == "AM") {
		Start[0] = 0
	}
	if(Start[0] != 12 && StartM[1] == "PM") {
		Start[0] = Start[0] + 12
		}
	if(End[0] != 12 && EndM[1] == "PM") {
		End[0] = End[0] + 12
		}
	//alert("Comparing " + Start[0] + " and " + hour)
	if(Start[0] <= hour && End[0] >= hour) {
		//alert("Ha!")
		if(Start[0] != End[0]) {
		//alert("Hm")
			if(End[0] > hour) {
			//alert("eh")
			return true;
			}
			if(End[0] == hour) {
			if(End[1] > minutes) {
			return true;
			}
			}
			}
		}
	else {
		return false;
	}
}
function UpdateTime() {
	date = new Date()
	day = date.getDay()
	hour = date.getHours()
	minutes = date.getMinutes()
	CheckFNT()
	CheckCAIN()
	}
setInterval(function(){ UpdateTime() }, 1000);
firstrunCAIN = 0
UpdateTime()
