function addShow(name, length) {
	table = document.getElementsByTagName("table")
	if(length == 100) {
		length = "210%"
		hourlength = "1"
	}
	if(length == 300) {
		length = "630%"
		hourlength = "3"
	}
	hourlength2 = parseInt(hourlength) * 2
	//table[0].innerHTML = table[0].innerHTML + "<td id='td" + hourlength + "'colspan='" + hourlength2 + "'>" + name + "</td>"
	//alert(table[0].innerHTML)
}
function left() {
	pageXOffset = window.pageXOffset;
	innerWidth = window.innerWidth;
	twenty = innerWidth * .7;
	if(pageXOffset > twenty) {
		window.scrollBy(twenty * -1, 0)
	}
	else {
		window.scrollTo(0, 0)
	}
}
function right() {
	pageXOffset = window.pageXOffset;
	innerWidth = window.innerWidth;
	twenty = innerWidth * .7;
	if(pageXOffset > twenty) {
		window.scrollBy(twenty, 0)
	}
	else {
		window.scrollTo(0, 0)
	}
}
</script>