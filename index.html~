audio = document.getElementsByTagName("audio")
current = document.getElementsByTagName("current")
state = document.getElementsByTagName("state")
next = document.getElementsByTagName("next")
nextstate = document.getElementsByTagName("nextstate")
main = document.getElementsByTagName("main")
timeTag = document.getElementsByTagName("time")
nextTime = document.getElementsByTagName("nexttime")
State = state[0].innerHTML
function Play(channel, url, img, desc) {
	main[0].id = ""
	current[0].innerHTML = channel
	audio[0].src = url;
	background[0].src = img
}
function report(status) {
	if(status == "load") {
		state[0].innerHTML = "<a id='a' href='javascript:pause()'>Loading | </a>" + desc
	}
	if(status == "play") {
		state[0].innerHTML = "<a id='a' href='javascript:pause()'>Playing | </a>" + desc
	}
	if(status == "pause") {
		state[0].innerHTML = "<a id='a' href='javascript:resume()'>Stopped | </a>" + desc
	}
	if(status == "buff") {
		state[0].innerHTML = "<a id='a' href='javascript:resume()'>Buffering | </a>" + desc
	}
}
function SetNext(channel, url, img, desc) {
	next[0].innerHTML = channel
	nextstate[0].innerHTML = desc
}
function pause() {
	audio[0].pause()
}
function resume() {
	audio[0].play()
}
function Tune(kall) {
	call = kall;
	if(call == "nrt1") {
		Play("<a id='link' href='javascript:Tune(\"nrt1\")'>" + nrt1now + "</a>", nrt1, "nrtback.jpg", nrt1desc)
		desc = nrt1desc
		SetNext("<a id='link' href='javascript:Tune(\"fnt\")'>" + fntnow + "</a>", fnt, "fntback.jpg", fntdesc)
	}
	if(call == "fnt") {
		Play("<a id='link' href='javascript:Tune(\"fnt\")'>" + fntnow + "</a>", fnt, "fntback.jpg", fntdesc)
		desc = fntdesc
		SetNext("<a id='link' href='javascript:Tune(\"cain\")'>" + cainnow + "</a>", cain, "cainback2.jpg", caindesc)
	}
	if(call == "cain") {
		Play("<a id='link' href='javascript:Tune(\"cain\")'>" + cainnow + "</a>", cain, "cainback2.jpg", caindesc)
		desc = caindesc
		SetNext("<a id='link' href='javascript:Tune(\"nrt1\")'>" + nrt1now + "</a>", nrt1, "nrtback.jpg", nrt1desc)
	}
}
main = document.getElementsByTagName("main")
guide = document.getElementsByTagName("guide")
function showGuide() {
	main[0].style.visibility = "hidden"
	guide[0].style.visibility = "visible"
	nowshown = "Guide"
	date = new Date()
	day = date.getDay()
	hour = date.getHours()
	minutes = date.getMinutes()
	table = document.getElementsByTagName("table")
	table[1].id="guide"
	if(day == 0) {
		location.href="#Sunday"
	}
	if(day == 1) {
		location.href="#Monday"
	}
	if(day == 2) {
		location.href="#Tuesday"
	}
	if(day == 3) {
		location.href="#Wednesday"
	}
	if(day == 4) {
		location.href="#Thursday"
	}
	if(day == 5) {
		location.href="#Friday"
	}
	if(day == 6) {
		location.href="#Saturday"
	}
	//alert("Oh!")
}
function showPlay() {
	main[0].style.visibility = "visible"
	guide[0].style.visibility = "hidden"
	table = document.getElementsByTagName("table")
	table[1].id="hideguide"
	nowshown = "Play"
	//alert("Oh.")
}
function toggleGuide() {
	justset = 0
	if(nowshown == "Play" && justset == 0) {
		//alert("Gah!")
		showGuide()
		justset = 1
	}
	else if(nowshown == "Guide" && justset == 0) {
		//alert("Gah.")
		showPlay()
		justset = 1
	}
}
Tune("nrt1")
guide = document.getElementsByTagName("guide")
table = document.getElementsByTagName("table")
